<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	</ui:style>
	
	<ui:with type="eu.nomme.resource.SiteResource" field="res"></ui:with>
	<g:HTMLPanel addStyleNames="{res.textContent.htmlPanel}" ui:field="htmlPanel">
		<g:Image addStyleNames="{res.textContent.textImage}" ui:field="image" />
		<g:HTML addStyleNames="{res.textContent.textBox}" ui:field="textBox" />
		
	</g:HTMLPanel>
</ui:UiBinder> 