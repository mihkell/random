<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	</ui:style>
	
	
	<ui:with type="eu.nomme.resource.SiteResource" field="res"></ui:with>
	<g:HTMLPanel  ui:field="htmlPanel">
	
		<h2 ui:field="titleH2"> <span addStyleNames="{res.catalogueRow.categoryName" ui:field="title"> </span> <hr /> </h2>
		<g:FlowPanel addStyleNames="{res.catalogueRow.itemsList}" ui:field="container"></g:FlowPanel>
		
	</g:HTMLPanel>
	
</ui:UiBinder> 