<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<ui:with type="eu.nomme.resource.SiteResource" field="res"></ui:with>
	<g:HTMLPanel addStyleNames="{res.catalogue.htmlPanel}" ui:field="htmlPanel">
	
		<g:HTML addStyleNames="{res.catalogue.topText}" ui:field="topText"></g:HTML>
		<g:VerticalPanel addStyleNames="{res.catalogue.gallery}" ui:field="gallery" />
		
	</g:HTMLPanel>
</ui:UiBinder> 