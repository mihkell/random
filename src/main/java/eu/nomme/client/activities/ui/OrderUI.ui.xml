<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>

	</ui:style>
	<ui:with type="eu.nomme.resource.SiteResource" field="res"></ui:with>
	<g:HTMLPanel addStyleNames="{res.order.htmlPanel}"
		ui:field="htmlPanel">

		<g:FlowPanel addStyleNames="{res.order.container}">

			<g:FlowPanel addStyleNames="{res.order.contact}"
				ui:field="contact">
				<g:HTML addStyleNames="{res.order.text}"
					text="CUSTOM ORDER">
				</g:HTML>

			</g:FlowPanel>

			<g:FlowPanel addStyleNames="{res.order.etsy}" ui:field="etsy">
				<g:HTML addStyleNames="{res.order.text}" text="ETSY"></g:HTML>
			</g:FlowPanel>

		</g:FlowPanel>
	</g:HTMLPanel>
</ui:UiBinder> 